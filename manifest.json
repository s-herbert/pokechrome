{
  "manifest_version": 2,

  "name": "POKEMON",
  "description": "YUMMMM",
  "version": "1.0",

  "background": {
    "scripts": ["scripts/background.js"],
    "persistent": false
  },

  "browser_action": {
   "default_title": "poke",
   "default_popup": "popup.html",
   "default_icon": {
    "16": "assets/pokeball16.png"
    }
  },



  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs",
    "activeTab",
    "storage"
  ]
}
